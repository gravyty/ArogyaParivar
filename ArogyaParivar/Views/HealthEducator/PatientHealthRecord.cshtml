    function bindData() {

        $.ajax({
            type: "POST",
            url: '@Url.Action("Allrgy")',
            data: "{}",
            contentType: "application/json; charset=utf-8",
            datatype: "json",
            async: "true",
            success: function (response) {

                if ($('#tblResult').length != 0) // remove table if it exists
                { $("#tblResult").remove(); }

                var table = "<table class='tblResult' id=tblResult><thead> <tr><th>Name</th><th>Email</th><th>Age</th></thead>  <tbody>";
                for (var i = 0; i <= response.d.length - 1; i++) {
                    var row = "<tr>";

                    row += '<td>' + response.d[i].Value + '</td>';
                    row += '<td>' + response.d[i].Email + '</td>';
                    row += '<td>' + response.d[i].Age + '</td>';
                    row += '</tr>';
                    table += row;
                }
                table += '</tbody></table>';
                $('#divData').html(table);
                $("#divData").slideDown("slow");
            },
            error: function (response) {
                alert(response.status + ' chandan ' + response.statusText);
            }
        });
    }

    function bindAllergyTable(response) {
        //personDataTable

        var thead = $("#personDataTable thead")[0].outerHTML;

        var tablerows = "";
        for (var i = 0; i <= response.length - 1; i++) {
            var row = "<tr>";
            row += '<td>' + (i + 1).toString() + '</td>';
            var date = getdate(response[i].CreateDate)
            row += '<td>' + date + '</td>';
            row += '<td>' + response[i].AllergyType + '</td>';
            row += '<td>' + response[i].ReactionName + '</td>';
            row += '<td>' + response[i].SeverityrName + '</td>';
            row += '<td>' + response[i].Status + '</td>';

            row += '</tr>';
            tablerows += row;
        }
        $("#personDataTable")[0].innerHTML = thead + tablerows;
    }

