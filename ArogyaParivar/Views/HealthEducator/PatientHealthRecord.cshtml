    function getdate(date) {
        //var MyDate_String_Value = 
        var value = new Date
                    (
                         parseInt(date.replace(/(^.*\()|([+-].*$)/g, ''))
                    );
        var date = value.getMonth() +
                                 1 +
                               "/" +
                   value.getDate() +
                               "/" +
               value.getFullYear();
        return date;
    }





