@model IEnumerable<ArogyaParivar.Models.DashBordModel>
@{
    ViewBag.Title = "DoctorDashBoard";
    Layout = "~/Views/Shared/_ArogyaParivarLayout.cshtml";
}

<h2>DoctorDashBoard</h2>
  <!-- Content Header (Page header) -->
    <section class="content-header">
       @*<%-- <h1>Dashboard</h1>*@
    @*    <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i>Home</a></li>
            <li><a href="#">Layout</a></li>
            <li class="active">Fixed</li>
        </ol>*@
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-lg-12">
                <div class="box box-widget widget-user" style="border:2px solid #f85c23">
                    <!-- Add the bg color to the header using any of the bg-* classes -->
                    <div class="widget-user-header bg-aqua-active" style="background-image:url(Images/arogyaparivarbg.png); background-repeat:no-repeat">
                    </div>
                    <div class="widget-user-image">
                        <img class="img-circle" src="~/Images/ArogyaParivarLogo.png" alt="Arogya Parivar Logo">
                    </div>
                    <div class="box-footer">
                        <div class="row">
                            <div class="col-lg-12" style="padding: 5px">
                                <div class="form-group">
                                    <div class="col-md-4">
                                        <input id="Text2" type="text" class="form-control" placeholder="Arogya ID" />
                                    </div>
                                    <div class="col-md-5">
                                        <input id="Text3" type="text" class="form-control" placeholder="Chief Complaint" />
                                    </div>
                                    <div class="col-md-3">
                                        <input id="Button1" type="button" value="Search" class="btn btn-warning" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    <!-- /.row -->
                </div>
              </div><!-- /.widget-user -->                
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <section class="col-lg-12 col-sm-12 connectedSortable">
                    <div class="box box-warning box-solid">
                        <div class="box-header with-border">
                            <h3 class="box-title">Patient Queue</h3>
                            <div class="box-tools pull-right">
                                <button class="btn btn-box-tool" data-widget="collapse">
                                    <i class="fa fa-minus"></i>
                                </button>
                            </div>
                            <!-- /.box-tools -->
                        </div>
                        <!-- /.box-header -->
                        <div class="box-body">
                            <div class="box-body table-responsive">
                                <table class="table table-bordered  table-hover">
                                    <thead>
                                                <tr>
                                                    
                                                    <th style="width: 10px">Token Number</th>
                                                    <th style="width: 10px">Patient ID</th>
                                                    <th>Date</th>
                                                    <th>Patient Name</th>
                                                    <th>Gender/Age</th>
                                                     <th>Complaint</th>
                                                </tr>
                                            </thead>
                                           @foreach (var item in Model) {
    <tr>
        <td>
                <a href="#" onclick="window.open('/Doctor/DoctorExamination?ArogyaID=@item.ArogyaID&TokenNumber=@item.TokenNumber');return false;">@item.TokenNumber</a>
            @*@Html.ActionLink(item.TokenNumber.ToString(), "DoctorExamination", "Doctor", new {item.TokenNumber,item.ArogyaID}, null)*@
           @* @Html.DisplayFor(modelItem => item.ArogyaID)*@
        </td>
        <td>
             @Html.DisplayFor(modelItem => item.ArogyaID)
        <td>
            @Html.DisplayFor(modelItem => item.TokenNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PatientName)
        </td>
        <td>
          @Html.DisplayFor(modelItem => item.GenderName)/@Html.DisplayFor(modelItem => item.Age)
        </td>
        
         <td>
            @Html.DisplayFor(modelItem => item.ComplaintName)
        </td>
    
    
    </tr>
                                           }
                                </table>
                            </div>
                        </div>
                        <!-- /.box-body -->
                    </div>
                    <!-- /.box -->
                </section>
         
            </div>
        </div>
    </section>